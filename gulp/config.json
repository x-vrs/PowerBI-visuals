{
    "uglifyJs": true,
    "nonminJs": true,
    "minifyCss": true,
    "generateMaps": true,
    "tslintOnBuild": true,
    "tslintOnChange": true,
    "emitTsLintError": true,
    "includeContentToMap": true,
    "showMSBuildFullLog": false,
    "showMSBuildLogOnError": true,
    "skipBuildOnInit": false,
    "enableIncrementalBuild": false,
    "sourceRootMapPrefix": "../ts/",
    "sourceRootMapFullPath": "",
    "cacheFileName": "gulp.tsc.cache",
    "paths": {
        "VisualsDropFolder": [
            "../lib/",
            "../src/Clients/PowerBIVisualsPlayground/lib"
        ],
        "VisualsDropStyles": [
            "../lib/",
            "../src/Clients/PowerBIVisualsPlayground/lib"
        ],
        "VisualsDropScripts": [
            "../lib/",
            "../src/Clients/PowerBIVisualsPlayground/lib"
        ],
        "VisualsDropImages": [
            "../lib/images/",
            "../src/Clients/PowerBIVisualsPlayground/lib/images/"
        ],
        "VisualsDropTests": "../VisualsTests"
    }
}